<?xml version = '1.0' encoding = 'UTF-8'?>
<oa:messageComponentLayout version="10.1.3_1312" xml:lang="en-US" xmlns:oa="http://xmlns.oracle.com/oa" xmlns:jrad="http://xmlns.oracle.com/jrad" xmlns:ui="http://xmlns.oracle.com/uix/ui" xmlns:user="http://xmlns.oracle.com/jrad/user" xmlns="http://xmlns.oracle.com/jrad" file-version="$Header$">
   <ui:contents>
      <oa:messageLayout id="confYesNo" prompt="Confirm Action">
         <ui:contents>
            <oa:button id="confYesBtn" text="Yes" prompt="Yes">
               <ui:primaryClientAction>
                  <ui:firePartialAction event="cnfYesBtn"/>
               </ui:primaryClientAction>
            </oa:button>
            <oa:button id="cnfNoBtn">
               <ui:primaryClientAction>
                  <ui:firePartialAction event="cnfNoBtn"/>
               </ui:primaryClientAction>
            </oa:button>
         </ui:contents>
      </oa:messageLayout>
   </ui:contents>
</oa:messageComponentLayout>
